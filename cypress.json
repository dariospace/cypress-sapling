{
  "screenshotsFolder": "artifacts/screenshots",
  "video": false,
  "videosFolder": "artifacts/videos",
  "videoUploadOnPasses": false,
  "viewportHeight": 660,
  "viewportWidth": 1300,
  "watchForFileChanges": true,
  "reporter": "mochawesome",
  "reporterOptions": {
    "reportDir": "artifacts/mochawesome-sources",
    "quiet": true,
    "overwrite": false,
    "html": false,
    "json": true
  },
  "env": {
    "abortStrategy": "run",
    "env": "stg",
    "isDebug": false
  }
}
